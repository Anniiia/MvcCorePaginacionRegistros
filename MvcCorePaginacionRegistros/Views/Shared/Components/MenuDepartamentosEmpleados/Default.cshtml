@model List<Departamento>
<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown">
        Departamentos
    </a>
    <ul class="dropdown-menu">
        @foreach (Departamento dept in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Paginacion" asp-action="EmpleadosDepartamentoOut" asp-route-iddepartamento="@dept.IdDepartamento">@dept.Nombre pag 1</a>

            </li>
            <li>
                <a class="dropdown-item" asp-controller="Paginacion" asp-action="EmpleadosDepartamentoOutDos" asp-route-iddepartamento="@dept.IdDepartamento">@dept.Nombre pag 2/3</a>
            </li>
        }
    </ul>
</li>